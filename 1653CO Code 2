campaignTrail_temp.cyoa = true

cyoAdventure = function (a) { 
    ans = campaignTrail_temp.player_answers[campaignTrail_temp.player_answers.length - 1]
    if (ans == 1736) {
       campaignTrail_temp.question_number= 34
      
    }
    else if (ans == 1737)
    {
        campaignTrail_temp.question_number = 40
    }
    else if (ans == 1749)
    {
        campaignTrail_temp.question_number = 40
    }
    else if (ans == 1748)
    {
        campaignTrail_temp.question_number = 37
    }
    else if (ans == 1691)
    {
        campaignTrail_temp.question_number = 38
    }
    else if (ans == 3244)
    {
        campaignTrail_temp.question_number = 39
    }
    else if (ans == 1823)
    {
        campaignTrail_temp.question_number = 40
    }
    else if (ans == 1827 || 1828)
    {
        campaignTrail_temp.question_number = 24
    }
    else if (ans == 30112 || 30113 || 30114)
    {
        campaignTrail_temp.question_number = 27
    }
    else if (ans == 1824 || 1825 || 1826)
    {
        campaignTrail_temp.question_number = 13
    }
    else 
    {
        return false
    }
}

